<div class="container-fluid">
   <div id="myCarousel" class="carousel slide bg-inverse w-100 ml-auto mr-auto" data-ride="carousel">
      <div class="carousel-inner" role="listbox">
         <div class="carousel-item active">
            <img class="d-block w-100" src="/images/carousel/banner1.png" height="500" alt="First slide">
            <div class="carousel-caption">
               {{!-- <h5 class="font-weight-light mb-4 letter-spacing-5">New Release</h5> --}}
               <h1 class="mb-5 display-2 font-weight-bold text-serif"></h1>
            </div>
         </div>
         <div class="carousel-item">
            <img class="d-block w-100" src="/images/carousel/banner2.jpg" height="500" alt="Second slide">
            <div class="carousel-caption">
               {{!-- <h5 class="font-weight-light mb-4 letter-spacing-5">New Release</h5> --}}
               <h1 class="mb-5 display-2 font-weight-bold text-serif"></h1>
            </div>
         </div>
         <div class="carousel-item">
            <img class="d-block w-100" src="/images/carousel/Homedecor1.jpg" height="500" alt="Third slide">
            <div class="carousel-caption">
               {{!-- <h5 class="font-weight-light mb-4 letter-spacing-5">New Release</h5> --}}
               <h1 class="mb-5 display-2 font-weight-bold text-serif"></h1>
            </div>
         </div>
      </div>
      <a class="carousel-control-prev" href="#myCarousel" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
      </a>
      <a class="carousel-control-next" href="#myCarousel" role="button" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
      </a>
   </div>
</div>
<div class="container-fluid" style="padding-top:70px;padding-bottom:70px">
   <div class="section-heading">
      <h2 class="section-title">Popular Products</h2>
   </div>
   {{# each products }}
   <div class="row">
      {{# each this }}
      <div class="col-lg-4 col-md-6">
         <div class="card text-center thumbnail ">
            <div class="card-content">
               <a href="/{{this.id}}/productDetails">
               <img class="imagesize" src="{{ this.imagepath}}" alt="{{ this.title }}">
               </a>
               <div class="card-body">
                  <h4 class="card-title">{{ this.title }}</h4>
                  <input type="hidden" id="custId" name="custId" value="{{this.productRating}}">
                  <div class="rating">
                     <div class="stars">
                        {{#times this.productRating}}
                        <span class="fa fa-star checked"></span>
                        {{/times}}
                        {{#ntimes this.productRating}}
                        <span class="fa fa-star"></span>
                        {{/ntimes}}
                     </div>
                  </div>
                  <p class="card-text">{{ this.description }}</p>
                  <div class="btn-group" role="group" aria-label="Basic example">
                     <span class="btn btn-outline-blue-grey">$ {{ this.price }}</span>
                     <a href="/add-to-cart/{{this.id}}/false/1" class="btn btn-success"> Add to Cart<i class="fa fa-shopping-cart"></i></a>
                  </div>
               </div>
            </div>
         </div>
      </div>
      {{/each}}  
   </div>
   {{/each}}
</div>
